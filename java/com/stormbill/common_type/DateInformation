package com.stormbill.stormbill.common_type;

public class BillTypes
{
	public String strName;
	public int nPicture;
	public int nBillTypeID;

	public BillTypes()
	{
		strName = new String();
		nPicture = 0;
		nBillTypeID = 0;
	}

	public void Instead(BillTypes other)
	{
		if(strName != other.strName)
			strName = other.strName;
		nPicture = other.nPicture;
		nBillTypeID = other.nBillTypeID;
	}
}
