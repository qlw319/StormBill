package com.stormbill.stormbill.bean;

public class AccountType
{
	private int nImg;
	private String strUpText;
	private String strDownText;

	public AccountType()
	{

		nImg = 0;
		strUpText = "";
		strDownText = "";
	}

	public int getImg()
	{
		return nImg;
	}

	public void setImg(int nImg)
	{
		this.nImg = nImg;
	}

	public String getUpText()
	{
		return strUpText;
	}

	public void setUpText(String strUpText)
	{
		this.strUpText = strUpText;
	}

	public String getDownText()
	{
		return strDownText;
	}

	public void setDownText(String strDownText)
	{
		this.strDownText = strDownText;
	}
}
